# include <stdio.h>
int main()
{
    int n;
    printf("Enter the choice:");
    scanf("%d",&n);
    switch(n)
    {
        case 1:
        printf("%d is the number",n);
        break;
        case 2:
        printf("The number is %d",n);
        break;
        default:
        printf("Its nothing");
    }
    return 0;
}